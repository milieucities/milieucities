# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*
!/log/.keep
/tmp

.byebug_history
/config/database.yml
dump.rdb
.env

node_modules
app/client/node_modules
npm-debug.log*
app/client/npm-debug.log*
Gemfile.lock
Makefile
*.DS_Store

# Ignore redis
/redis-stable

# Ignore application configuration
/config/application.yml

# Ignore public folder
/public/assets
